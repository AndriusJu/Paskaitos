<Window x:Class="Kraujo_Bankas.MainWindow"
        WindowStartupLocation="CenterScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kraujo_Bankas"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
        
    <Grid>
        <TextBlock Text="Sveiki atvyke į Kraujo banka" FontSize="20" Margin="0 -200 0 0"  Height="50" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                
            <Button x:Name="Prisijungti" Content="Prisijungti" FontSize="15" Margin=" 0 0 0 0" Width="200" Height="50" HorizontalAlignment="Center" VerticalAlignment="Center" Click="Pasirinkimas_Prisijungti_Click"/>
      
            <Button x:Name="Registruotis" Content="Registruotis" FontSize="15" Margin="0 200 0 0" Width="200" Height="50" HorizontalAlignment="Center" VerticalAlignment="Center" Click="Registruotis_Click"/>
        
    </Grid>
</Window>
